{
  "version": 2,
  "name": "cafecito-tech",
  "public": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" }
      ]
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true
}
